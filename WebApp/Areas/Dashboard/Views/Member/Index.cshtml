@model IEnumerable<Member>
<table class="table table-bordered">
    <thead class="table-dark">
        <tr>
            <th>MemberId</th>
            <th>Name</th>
            <th>Given Name</th>
            <th>Surname</th>
            <th>Email</th>
            <th>Role</th>
            <th>Command</th>
        </tr>
    </thead>
    @foreach(var item in Model){
       <tr>
            <td>@item.MemberId</td>
            <td>@item.Name</td>
            <td>@item.GivenName</td>
            <td>@item.Surname</td>
            <td>@item.Email</td>
            <td>@item.Role</td>
            <td>
                <a href="/dashboard/member/updaterole/@item.MemberId" class="btn btn-warning btn-sm">Update Role</a>
                <a href="/dashboard/member/delete/@item.MemberId" class="btn btn-danger btn-sm" onclick="return confirm('Are you sure you want to delete this member?');">Delete</a>
            </td>
       </tr>
    }
</table>